{
  "name": "socket-json-wrapper",
  "version": "1.0.1",
  "description": "Wrapper for socket connections that allows you to send and receive json objects.",
  "main": "index.js",
  "typings": "index.d.ts",
  "scripts": {
    "test": "rm -f test-socket && rm -rf coverage && nyc mocha",
    "lint": "node_modules/eslint/bin/eslint.js ."
  },
  "pre-push": [
    "lint",
    "test"
  ],
  "engines": {
    "node": ">=8.0.0"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/robojones/socket-json-wrapper.git"
  },
  "keywords": [
    "socket",
    "connection",
    "json",
    "net",
    "tcp"
  ],
  "author": "robojones",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/robojones/socket-json-wrapper/issues"
  },
  "homepage": "https://github.com/robojones/socket-json-wrapper#readme",
  "nyc": {
    "exclude": [
      "test/*"
    ],
    "reporter": [
      "lcov",
      "text-summary"
    ]
  },
  "devDependencies": {
    "codeclimate-test-reporter": "^0.5.0",
    "eslint": "^4.7.1",
    "eslint-config-ultra": "^0.4.5",
    "mocha": "^4.0.0",
    "nyc": "^11.2.1",
    "pre-push": "^0.1.1"
  },
  "dependencies": {
    "@types/node": "^9.4.6"
  }
}
